<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">

    <welcome-file-list>
        <welcome-file>classes\html\index.html</welcome-file>
    </welcome-file-list>

    <servlet>
    <servlet-name>FirstServlet</servlet-name>
    <servlet-class>FirstServlet</servlet-class>
</servlet>
    <servlet-mapping>
        <servlet-name>FirstServlet</servlet-name>
        <!--servlet访问地址,为动态资源地址-->
        <url-pattern>/firstServlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>AddFeatureServlet</servlet-name>
        <servlet-class>servlets.AddFeatureServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>AddFeatureServlet</servlet-name>
        <!--servlet访问地址,为动态资源地址-->
        <url-pattern>/addFeatureServlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>ProvStaServlet</servlet-name>
        <servlet-class>servlets.ProvStaServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ProvStaServlet</servlet-name>
        <!--servlet访问地址,为动态资源地址-->
        <url-pattern>/provStaServlet</url-pattern>
    </servlet-mapping>
</web-app>